{
    "name": "smartdata-co/online-stone-sdk-php",
    "description": "SDK Online Stone Integração",
    "type": "library",
    "license": "GPLv3",
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ],
        "coverage": [
            "vendor/bin/phpunit --coverage-html tests/_reports"
        ],
        "cs": [
            "vendor/bin/phpcs src/ tests/ --standard=PSR2"
        ],
        "cc": [
            "vendor/bin/psalm"
        ]
    },
    "autoload": {
        "psr-4": {
            "SmartdataCo\\OnlineStoneSdkPhp\\": "src/SmartdataCo/OnlineStoneSdkPhp"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SmartdataCo\\OnlineStoneSdkPhp\\Test\\": "tests/unit"
        }
    },
    "authors": [
        {
            "name": "Iuri Andreazza",
            "email": "iuri.andreazza@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.17.0",
        "squizlabs/php_codesniffer": "^3.7",
        "vimeo/psalm": "^4.27"
    },
    "require": {
        "guzzlehttp/guzzle": "^7.5",
        "pear/xml_serializer": "^0.22.0"
    }
}
